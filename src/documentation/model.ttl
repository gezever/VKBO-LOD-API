adms:Identifier
        skos:notation  "http://todo.ex/vestigingsnummer" .


skos:Concept  rdf:type  "skos:Concept" .


geo:Geometry
        geo:asWKT             "http://www.opengis.net/ont/geosparql#wktLiteral";
        geo:hasSerialization  "http://www.opengis.net/ont/geosparql#wktLiteral" .

org:OrganizationalUnit
        geo:hasGeometry               geo:Geometry;
        skos:altLabel                 "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString";
        skos:prefLabel                "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString";
        adms:identifier               adms:Identifier;
        locn:address                  locn:Address;
        org:classification            skos:Concept;
        org:siteOf                    org:FormalOrganization ;
        org:subOrganizationOf         org:FormalOrganization ;
        org:transitiveSubOrganizationOf
                org:FormalOrganization ;
        org:unitOf                    org:FormalOrganization ;
        regorg:legalName              "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString";
        regorg:orgActivity            skos:Concept;
        regorg:registration           adms:Identifier;
        organisatie:oprichtingsdatum  "http://www.w3.org/2001/XMLSchema#dateTime" .


locn:Address
        locn:geographicName  "http://www.w3.org/2001/XMLSchema#string";
        locn:postCode        "http://www.w3.org/2001/XMLSchema#string";
        ad:busnummer         "http://www.w3.org/2001/XMLSchema#string";
        ad:huisnummer        "http://www.w3.org/2001/XMLSchema#string";
        ad:straatnaam        "http://www.w3.org/2001/XMLSchema#string" .

org:FormalOrganization
        org:hasPrimarySite      org:OrganizationalUnit ;
        org:hasRegisteredSite   org:OrganizationalUnit ;
        org:hasSite             org:OrganizationalUnit ;
        org:hasSubOrganization  org:OrganizationalUnit ;
        org:hasUnit             org:OrganizationalUnit .


